{
  "name": "mekras/odata-client",
  "description": "OData client-side library",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "OData"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Михаил Красильников",
      "email": "m.krasilnikov@yandex.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Mekras\\OData\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mekras\\OData\\Client\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6",
    "mekras/atompub": "^1.0",
    "php-http/client-implementation": "^1.0"
  },
  "require-dev": {
    "php-http/curl-client": "^1.5.1",
    "phpunit/phpunit": "^5.0.9",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}